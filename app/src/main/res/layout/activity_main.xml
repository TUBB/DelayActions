<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="com.tubb.delayactions.test.MainActivity">
    <Button
        android:id="@+id/btn_order_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="to order detail page"
        android:onClick="viewClick"/>
    <Button
        android:id="@+id/btn_clear_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="clear login info"
        android:onClick="viewClick"/>
    <Button
        android:id="@+id/btn_clear_discount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="clear discount info"
        android:onClick="viewClick"/>
</LinearLayout>
